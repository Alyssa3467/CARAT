{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "mocha": true,
        "mongo": true
    },
    "extends": [
        "eslint:recommended",
        "google",
        "prettier"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "strict": "error",
        "require-atomic-updates": "error",
        "no-magic-numbers": "warn",
        "no-console": "warn",
        "no-extra-parens": "warn"
    }
}